{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "clean": {
            "cache": false
        },
        "lint": {}
    }
}